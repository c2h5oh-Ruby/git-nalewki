<!DOCTYPE html>
<html>
<head>
  <title>Nalewki</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= ajaxful_rating_style %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="container">
<div id="topPan">
	<img src="../images/logo.gif" title="Liqidity Graph" alt="Liqidity Graph" width="260" height="56" border="0" class="logo" />
	<ul>
	<li><% if user_signed_in? %>
	     <%= link_to "Wyloguj", destroy_user_session_path %>
         <% else %> 
		 <%= link_to_rpx "Zaloguj", user_session_url %>
		 <% end %> 
	</li>
		<li><%= link_to "Katalog", recipes_path %></li>
		<li><%= link_to 'Dodaj', new_recipe_path %></li>

	</ul>
</div>


<%= yield %>
<%= javascript_include_rpx(user_session_url) %>
</body>
</html>
